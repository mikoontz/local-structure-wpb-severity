  keywords = {ASD FieldSpec measurements,Biological system modeling,biomass reduction,canopy chlorophyll content,canopy nitrogen content,Chlorophyll,Cropscan radiometer,geochemistry,grazed fen meadow,Hyperion data,hyperspectral,Hyperspectral imaging,hyperspectral reflectance data,hyperspectral remote sensing data,Indexes,leaf area index,MERIS data,nitrogen,Nitrogen,plant photosynthetic capacity,plant pigment system,plant stress,potato field,PROSAIL,PROSAIL radiative transfer model,PROSAIL simulations,red-edge chlorophyll index,remote sensing,remote sensing techniques,Sentinel-2 system,Sentinel-3 system,spectroscopy,vegetation,vegetation mapping,Vegetation mapping},
  number = {2},
  title = {Using Hyperspectral Remote Sensing Data for Retrieving Canopy Chlorophyll and Nitrogen Content}
}


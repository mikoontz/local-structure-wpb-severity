@article{fettig2014,
  author = {Fettig, Christopher J. and Gibson, Kenneth E. and Munson, A. Steven and Negr\'on, Jos\'e F.},
  date = {2014-06-19},
  journaltitle = {Forest Science},
  volume = {60},

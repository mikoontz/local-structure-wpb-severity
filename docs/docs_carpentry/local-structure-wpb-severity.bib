  file = {/Users/michaelkoontz/Zotero/storage/XVMVXP83/Preisler - 1993 - Modelling Spatial Patterns of Trees Attacked by Ba.pdf},
  langid = {english},
  number = {3},
  title = {Modelling Spatial Patterns of Trees Attacked by Bark-Beetles}
}

